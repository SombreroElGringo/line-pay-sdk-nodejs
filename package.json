{
  "name": "line-pay-sdk",
  "version": "1.0.3",
  "description": "Node.js SDK for LINE Pay",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "docs",
    "lib"
  ],
  "scripts": {
    "test": "TEST_PORT=1234 nyc mocha",
    "prettier": "prettier --parser typescript --trailing-comma all \"{lib,test}/**/*.ts\"",
    "format": "npm run prettier -- --write",
    "format:check": "npm run prettier -- -l",
    "clean": "del-cli dist",
    "build": "tsc",
    "docs": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:deploy": "./deploy-docs.sh",
    "release": "npm run build && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SombreroElGringo/line-pay-sdk-nodejs.git"
  },
  "keywords": [
    "node",
    "line",
    "sdk"
  ],
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.10",
    "@types/node": "^10.17.17",
    "axios": "^0.19.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "del-cli": "^1.1.0",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "prettier": "^1.14.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.8.3",
    "vuepress": "^0.14.11"
  },
  "nyc": {
    "require": [
      "ts-node/register"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SombreroElGringo/line-pay-sdk-nodejs/issues"
  },
  "homepage": "https://github.com/SombreroElGringo/line-pay-sdk-nodejs#readme"
}
